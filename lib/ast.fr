@pub fn new_call_ast(f: AstExpr, arg: AstExpr) AstExpr = f!unquote(arg!unquote)!quote;
@pub fn new_pair_ast(a: AstExpr, b: AstExpr) AstExpr = (a!unquote, b!unquote)!quote;



