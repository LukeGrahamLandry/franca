// Error Quality
main :: fn() void = {
    s := "a";
    a: ?i64 = .None;
    @match(a) {
        fn Some() => ();
        // because '=' instead of '=>' it doesn't capture the scope. 
        // @match should report that as an error instead of letting it through until:
        // "Compile Error: missing runtime var s"
        fn None() = {
            println(s);
        }
    };
}
