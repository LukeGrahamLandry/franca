# i only inline things if declared before called. 
# $b can't be inlined into $a but $a can be inlined into $main 
# at which point $b could be inlined if i looked at the instructions again. 

function l $a(l %a) {
@0
    %b =l call $b(l %a)
    ret %b
}

function l $b(l %a) {
@0
    ret %a
}

export function l $main() {
@0
    %a =l call $a(l 0)
    ret %a
}
