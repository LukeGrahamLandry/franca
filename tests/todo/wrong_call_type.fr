main :: fn() = {
    foo :: fn(a: i64) i64 = {
        a * 2
    };
    use_foo :: fn($_unused: Type, $f: @Fn(a: i64) Str) void = {
        s := f(123);
        println(s);
    };
    // shouldn't type check because use_foo wanted it to return Str. 
    // and doesn't if you remove the  _unused param. 
    // this leads to the expr Fn type and the func's type being different when you get to emit_ir
    // i guess normally i rely on overload resolution to do it so it doesn't work if you pass a func directly. 
    // example in the real program: native_abi.selcall expects return void and i pass it wasm.sel_call which returns *Ins. 
    use_foo(i64, foo);
}
