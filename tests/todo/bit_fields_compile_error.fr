// :BitFieldCompileError
main :: fn() void = {
    a := zeroed B;
    @println("%", a&);
}

A :: @bit_fields(
    a := 5,
    b := 5,
    c := 6,
);

B :: @struct {
    a: A;
};

fn display(a: *B, out: *List(u8)) void = {
    a := a.a;
    @fmt(out, "% % %", a.get(.a), a.get(.b), a.get(.c));
}

#use("@/lib/bit_fields.fr");
