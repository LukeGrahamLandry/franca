
#use("@/examples/mandelbrot.fr")
fn count_mandelbrot() #test = {
    mandelbrot := render();
    count := 0;
    for mandelbrot.items() { c | 
        count += int(c == "@".ascii());
    };
    @assert_eq(count, 883, "i don't think that worked");
}

