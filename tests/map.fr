#test fn test_option() void = {
    var s: ?i64 = some(123);
    
    var a = 0;
    s.if(fn(i: i64) void => {
        a = i;
    });
    
    assert_eq(a, 123);
    
    var i = 0;
    while(=> {s}) { (v: i64) |
        s = none();
        i = i.add(1);
    };
    assert_eq(i, 1);
    
    let n: ?i64 = none();
    let z = n.or(=> 555);
    assert_eq(z, 555);
    
    var y: ?i64 = some(10);
    let z = y.or(=> 555);
    assert_eq(z, 10);
    
    assert_eq(true, y.is_some());
    y.unwrap();
    
    let y_ptr = y&.as_ref();
}

