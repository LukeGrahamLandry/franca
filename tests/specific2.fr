@hacktest(interp, aarch64)
fn varient(c: i64) i64 = {
    fn inner(n: i64) i64 = {
        const A = (a: i64, b: i64)!enum;
        var a: A = (a: n);
        add(a&.a[], 7)
    }
    
    assert_eq(inner(3), 10);
    c
}

// Requires being able to write constant heap values.
@hacktest(interp, aarch64)
fn use_str(c: i64) i64 = {
    let s: Str = "hello";
    assert_eq(len(s), 5);
    c
}

@test(interp, aarch64)
fn main(c: i64) i64 = {
    assert_eq(c, varient(c));
    assert_eq(c, use_str(c));
    c
}
