// TODO: llvm can't return multiple values yet. 
// Requires being able to write constant heap values.
@hacktest(aarch64)
fn use_str(c: i64) i64 = {
    let s: Str = "hello";
    assert_eq(len(s), 5);
    c
}


@test(aarch64)
fn main(c: i64) i64 = {
    assert_eq(c, use_str(c));
    c
}
