// These work on the new qbe based backend but not the old comptime jit. 

fn big_stack_frame() void #test = {
    S :: @struct(
        a: Array(u8, 5000),
    );
    fn do_something_important(s: *S) i64 #noinline = {
        a: i64 = s.a&[0].zext();
        b: i64 = s.a&[1].zext();
        c: i64 = s.a&[2].zext();
        if(a == 0, => b, => c)
    }
    s := S.zeroed();
    c := s&.do_something_important();
    assert_eq(c, 0);
}
