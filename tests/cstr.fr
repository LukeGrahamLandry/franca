::ptr_utils(u8);
    
#test(aarch64)
fn main(canary: i64) i64 = {
    a := "Hello World".str();
    b := "Hello World"!symbol.c_str();
    
    range(0, a.len()) {(i: i64) Unit|
        a_char := a.get(i);
        b_char := b.index(i).load();
        c := a_char.eq(@as(i64) b_char);
        assert(c, "fail");
    };
    
    var i = 0;
    for(b) {(b_char: u8)|
        a_char := a.get(i);
        c := a_char.eq(@as(i64) b_char);
        assert(c, "fail");
        i = i.add(1);
    };
    
    aa := b.to_wide();
    assert_eq(true, eq(a, aa&.items()));
    
    canary
}
