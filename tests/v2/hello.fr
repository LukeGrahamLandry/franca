
builtin   :: "this will never be evaluated... but it needs to resolve (HACK)";
simple_eq :: @builtin("compiler_debug_assert_eq_i64");

#test fn call_a_builtin() = {
    abc :: 123; 
    simple_eq(abc, 123);
}

#test fn read_var() = {
    a := 456; 
    simple_eq(a, 456);
}

#test fn set_var() = {
    a := 456; 
    a = 789;
    simple_eq(a, 789);
}
