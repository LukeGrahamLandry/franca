fn loadopt(_0: *Qbe.Fn) void #import("qbe");

fn storesz(i: *Qbe.Ins) i32 = {
    @match(i.op()) {
        fn storeb() => 1;
        fn storeh() => 2;
        fn storew() => 4;
        fn stores() => 4;
        fn storel() => 8;
        fn stored() => 8;
        @default => panic("invalid op for storesz");
    }
}

fn loadsz(i: *Qbe.Ins) i32 = {
    @match(i.op()) {
        fn loadsb() => 1;
        fn loadub() => 1;
        fn loadsh() => 2;
        fn loaduh() => 2;
        fn loadsw() => 4;
        fn loaduw() => 4;
        fn load() => if(i.cls().is_wide(), => 8, => 4);
        @default => panic("invalid op for loadsz");
    }
}
