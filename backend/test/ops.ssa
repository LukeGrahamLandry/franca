data $fmt = { b "swap:  %#010x %#010x\nrot8:  %#010x %#010x\nrot12: %#010x %#010x\nover:  %#010x %#010x\n" }

export
function w $main() {
@start
    call $run(w 305419896, w 12)
	ret 0
}

export
function $run(w %a, w %twelve) {
@start
    %bytes =w byteswap %a
    %r8  =w rotr %a, 8
    %l8  =w rotl %a, 8
    %r12 =w rotr %a, %twelve
    %l12 =w rotl %a, %twelve
    %lover =w rotr %a, %a
    %rover =w rotl %a, %a
    call $printf(l $fmt, ..., w %a, w %bytes, w %l8, w %r8, w %l12, w %r12, w %lover, w %rover)
	ret
}

