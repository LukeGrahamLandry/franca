# this calls two dynamic imports. 
# so you need to be able to encode the (Mach-O) fixup chain correctly. 

export function w $main() {
@start
    %a =l call $strlen(l $s)
    %b =w add 11, 2
    %c =w cnew %a, %b
    %_ =w call $printf(l $fmt, ..., w %a, w %b)
    ret %c
}
data $s = { b "elevenplustwo", b 0 }
data $fmt = { b "2 + 11 = %d, %d\n", b 0 }

# >>> output
# 2 + 11 = 13, 13#
# <<<
