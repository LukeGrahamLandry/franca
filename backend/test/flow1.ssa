# this has a loop that can't end immediately after the last jump to it
export function w $main() {
 @A
    jmp @B
 @B
    %a =l phi @A 0, @I %b
    %c =l csltl %a, 1
    jnz %c, @C, @D
 @C
    jmp @E
 @D
    ret 0
 @E
    jnz %a, @F, @G
 @F
    jmp @H
 @G
    %b =l add %a, 1
    jnz %b, @I, @J
 @H
    jmp @E
 @I
    jmp @B
 @J
    jmp @H
}
