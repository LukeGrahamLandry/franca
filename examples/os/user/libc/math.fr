
fabs :: fn(val: f64) f64 #static = 
    @if(val < 0, -val, val);

sqrt :: fn(a: f64) f64 #ir(.sqrt, .Kd);
sqrtf :: fn(a: f32) f32 #ir(.sqrt, .Ks);
fmin :: fn(a: f64, b: f64) f64 #ir(.min, .Kd);
fminf :: fn(a: f32, b: f32) f32 #ir(.min, .Ks);
fmax :: fn(a: f64, b: f64) f64 #ir(.max, .Kd);
fmaxf :: fn(a: f32, b: f32) f32 #ir(.max, .Ks);
