driver :: fn(vtable: *ImportVTable) void = {
    franca := get_executable_path(temp());
    build :: fn(src, dest) => 
        run_cmd_blocking(franca, @slice("examples/default_driver.fr", "build", src, "-o", dest, "-arch", "wasm32"));
    
        build("examples/kaleidoscope.fr", "examples/web/a.wasm");
        build("examples/web/b.fr", "examples/web/b.wasm");
        build("examples/web/c.fr", "examples/web/c.wasm");
}

