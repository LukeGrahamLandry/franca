
main :: fn() void = {
    s := import("@/lib/sys/fs.fr")'read_file_from_last_arg();
    s := s.split("\n", temp()).items();
    one, two := (0, 0);
    x := 50;
    
    for s { s |
        dir := 1 - 2*s.starts_with("L").int();
        clicks := s.rest(1).parse_int();
        range(0, clicks) { _ |
            x = x.add(dir).mod(100);
            two += int(x == 0);
        };
        one += int(x == 0);
    };
    
    println(one);
    println(two);
}
