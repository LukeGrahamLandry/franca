::enum(Token);
::tagged(KExpr);

EXAMPLE_PROGRAM :: """
# mandel from https://llvm.org/docs/tutorial/MyFirstLanguageFrontend/LangImpl06.html#kicking-the-tires
# fib from https://llvm.org/docs/tutorial/MyFirstLanguageFrontend/LangImpl07.html#id5
# (retrieved April 20, 2025)

extern printd(x);
extern putchard(char);

def unary-(v)
  0-v;

def binary> 10 (LHS RHS)
  RHS < LHS;

def binary| 5 (LHS RHS)
  if LHS then
    1
  else if RHS then
    1
  else
    0;

def binary : 1 (x y) y;

def printdensity(d)
  if d > 8 then
    putchard(32)  # ' '
  else if d > 4 then
    putchard(46)  # '.'
  else if d > 2 then
    putchard(43)  # '+'
  else
    putchard(42); # '*'

def mandelconverger(real imag iters creal cimag)
    if iters > 255 | (real*real + imag*imag > 4) then
    iters
    else
    mandelconverger(real*real - imag*imag + creal,
                    2*real*imag + cimag,
                    iters+1, creal, cimag);

def mandelconverge(real imag)
    mandelconverger(real, imag, 0, real, imag);

def mandelhelp(xmin xmax xstep   ymin ymax ystep)
    for y = ymin, y < ymax, ystep in (
    (for x = xmin, x < xmax, xstep in
        printdensity(mandelconverge(x,y)))
    : putchard(10)
    )

def mandel(realstart imagstart realmag imagmag)
    mandelhelp(realstart, realstart+realmag*78, realmag,
                imagstart, imagstart+imagmag*40, imagmag);

mandel(-2.3, -1.3, 0.05, 0.07);

def fib(x)
  if (x < 3) then
    1
  else
    fib(x-1)+fib(x-2);

def fibi(x)
  var a = 1, b = 1, c in
  (for i = 3, i < x in
     c = a + b :
     a = b :
     b = c) :
  b;

fib(10);
fibi(10);
""";
