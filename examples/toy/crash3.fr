// franca examples/default_driver.fr build examples/toy/crash3.fr -keep-names -debug-info && FRANCA_BACKTRACE=1 ./a.out

fn main() void = {
    Crash :: import("@/lib/crash_report.fr");
    Crash'hook_backtrace();
    println("C");
    hello(1);
    println("D");
}

fn hello(a: i64) void #inline = {
    println("A");
    world();
    //println("B");
}

fn world() void #noinline = {
    println("Z");
    @assert_eq(0, 1);
}
