#!/usr/bin/env franca
// List the files in the current directory. 
// This is exciting because on macos-amd64, you some symbol names have `$INODE64` appended. 

fn main() = {
    entries := collect_directory_ordered(".", temp(), temp()) 
        || @panic("failed walkdir");
    for entries { entry |
        println(entry.name);
    };
}

